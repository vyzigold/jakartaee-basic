<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<navigation-rule>
		<display-name>person_list.xhtml</display-name>
		<from-view-id>/person_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/person_new.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_new.xhtml</display-name>
		<from-view-id>/person_new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/person_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_list.xhtml</display-name>
		<from-view-id>/person_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/person_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_list.xhtml</display-name>
		<from-view-id>/person_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/person_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_edit.xhtml</display-name>
		<from-view-id>/person_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/person_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_edit.xhtml</display-name>
		<from-view-id>/person_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newcar</from-outcome>
			<to-view-id>/car_add.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>car_add.xhtml</display-name>
		<from-view-id>/car_add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/person_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>person_edit.xhtml</display-name>
		<from-view-id>/person_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/person_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/admin/admin.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>admin/admin.xhtml</display-name>
		<from-view-id>/admin/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
